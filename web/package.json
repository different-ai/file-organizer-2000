  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm next dev",
    "build:self-host": "pnpm next build",
    "build": "pnpm run db:migrate && npx @u22n/srm deploy && pnpm next build",
    "start": "pnpm next start",
    "lint": "pnpm next lint",
    "db:generate": "pnpm drizzle-kit generate",
